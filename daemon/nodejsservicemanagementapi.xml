<service>
    <id>nodejsservicemanagementapi.exe</id>
    <name>Node.js Service Management API</name>
    <description>The nodejs.org service management api.</description>
    <executable>C:\Program Files\nodejs\node.exe</executable>
    <logmode>rotate</logmode>
    <argument>--harmony</argument>
    <argument>C:\Users\WILLIAM\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
    <argument>--file</argument>
    <argument>C:\laragon\www\lachamelia-api\server.js</argument>
    <argument>--log</argument>
    <argument>Node.js Service Management API wrapper</argument>
    <argument>--grow</argument>
    <argument>0.25</argument>
    <argument>--wait</argument>
    <argument>1</argument>
    <argument>--maxrestarts</argument>
    <argument>3</argument>
    <argument>--abortonerror</argument>
    <argument>n</argument>
    <argument>--stopparentfirst</argument>
    <argument>undefined</argument>
    <stoptimeout>30sec</stoptimeout>
    <workingdirectory>C:\laragon\www\lachamelia-api</workingdirectory>
</service>
